{% extends "core/base.jinja" %}

{% block title %}
  {% trans %}Page list{% endtrans %}
{% endblock %}

{% block content %}
  <h3>{% trans %}Page list{% endtrans %}</h3>
  <ul>
    {% for p in page_list %}
      <li><a href="{{ p.get_absolute_url() }}">{{ p.display_name }}</a></li>
    {% endfor %}
  </ul>
{% endblock %}



